{
  "name": "vscode-to-vs",
  "displayName": "VSCode to Visual Studio",
  "publisher": "PostGuitarist",
  "description": "Creates solution files for a Visual Studio C++ project with just source files.",
  "author": {
    "name": "Zaden Connell"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/vsicon.png",
  "pricing": "Free",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PostGuitarist/vscode-to-vs-extension.git"
  },
  "bugs": {
    "url": "https://github.com/PostGuitarist/vscode-to-vs-extension/issues"
  },
  "keywords": [
    "vscode",
    "vs",
    "visual",
    "studio",
    "code",
    "cpp",
    "c++",
    "solution",
    "project",
    "files",
    "generate",
    "create",
    "extension"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateCppSolution",
        "title": "Generate C++ Solution Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.23.0"
  }
}
