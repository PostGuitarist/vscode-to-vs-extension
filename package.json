{
  "name": "vscode-to-vs",
  "displayName": "VSCode to Visual Studio",
  "publisher": "PostGuitarist",
  "description": "Creates solution files for a Visual Studio C++ project with just source files.",
  "author": {
    "name": "Zaden Connell"
  },
  "version": "1.6.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/vsicon.png",
  "pricing": "Free",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/PostGuitarist/vscode-to-vs-extension.git"
  },
  "bugs": {
    "url": "https://github.com/PostGuitarist/vscode-to-vs-extension/issues"
  },
  "keywords": [
    "vscode",
    "vs",
    "visual",
    "studio",
    "code",
    "cpp",
    "c++",
    "solution",
    "project",
    "files",
    "generate",
    "create",
    "extension"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-to-vs.generateCppSolution",
        "title": "Generate C++ Solution Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
